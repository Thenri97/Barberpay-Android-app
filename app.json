{
  "expo": {
    "name": "BarberPay",
    "slug": "teste-barberpay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/BarberPay.png",
    "scheme": "barberpay",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thenri.testebarberpay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/BarberPay.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.thenri.testebarberpay"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/BarberPay.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "20c94af4-49f1-4fd8-b9cc-e7dd843dc988"
      }
    }
  }
}
